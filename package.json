{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:dev": "nodemon server.ts",
    "start:prod": "set \"NODE_ENV=production\" && ts-node server.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/ssrComponents/serverSide.tsx",
    "lint": "eslint . --ext .tsx",
    "prettier": "npx prettier --write .",
    "test:unit": "vitest",
    "test:coverage-unit": "vitest run --coverage",
    "test:e2e": "cypress open --e2e --browser chrome",
    "test:coverage-e2e": "cypress run --e2e & npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "esbuild": "0.17.17",
    "history": "^5.3.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/react18": "^2.0.0",
    "@hookform/error-message": "^2.0.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress": "^1.1.3",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.1",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "cypress": "^12.11.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "sass": "^1.60.0",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "typescript-plugin-css-modules": "^5.0.1",
    "vite": "^4.2.0",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3"
  }
}
